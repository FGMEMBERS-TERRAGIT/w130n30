<?xml version="1.0" encoding="UTF-8"?>

<PropertyList>

    <path>KOAK_tower.ac</path>

    <animation>
	<type>material</type>
	<object-name>Windows</object-name>
	<transparency>
	    <factor>-0.05</factor>
	    <offset>0.2</offset>
	    <alpha-prop>rendering/scene/diffuse/blue</alpha-prop>
	</transparency>
	<emission>
	    <factor>-1</factor>
	    <offset>0.5</offset>
	    <red-prop>rendering/scene/diffuse/blue</red-prop>
	    <green>1</green>
	    <blue>1</blue>
	</emission>
    </animation>

    <animation>
	<type>select</type>
	<object-name>RedSpot.001</object-name>
	<object-name>RedSpot.002</object-name>
	<object-name>RedSpot.003</object-name>
	<object-name>RedSpot.004</object-name>
	<condition>
	    <greater-than>
		<property>/sim/time/sun-angle-rad</property>
		<value>1.57</value>
	    </greater-than>
	</condition>
    </animation>

    <animation>
	<type>alpha-test</type>
	<object-name>RedSpot.001</object-name>
	<object-name>RedSpot.002</object-name>
	<object-name>RedSpot.003</object-name>
	<object-name>RedSpot.004</object-name>
    </animation>

    <animation>
	<type>noshadow</type>
	<object-name>RedSpot.001</object-name>
	<object-name>RedSpot.002</object-name>
	<object-name>RedSpot.003</object-name>
	<object-name>RedSpot.004</object-name>
    </animation>

    <animation>
	<type>translate</type>
	<object-name>RedSpot.001</object-name>
	<offset-m>70.60</offset-m>
	<axis>
	    <x>-1.82</x>
	    <y>2.05</y>
	    <z>70.55</z>
	</axis>
    </animation>
    <animation>
	<type>translate</type>
	<object-name>RedSpot.002</object-name>
	<offset-m>70.61</offset-m>
	<axis>
	    <x>1.95</x>
	    <y>2.13</y>
	    <z>70.55</z>
	</axis>
    </animation>
    <animation>
	<type>translate</type>
	<object-name>RedSpot.003</object-name>
	<offset-m>70.60</offset-m>
	<axis>
	    <x>1.95</x>
	    <y>-1.85</y>
	    <z>70.55</z>
	</axis>
    </animation>
    <animation>
	<type>translate</type>
	<object-name>RedSpot.004</object-name>
	<offset-m>70.60</offset-m>
	<axis>
	    <x>-2.00</x>
	    <y>-1.86</y>
	    <z>70.55</z>
	</axis>
    </animation>

    <animation>
	<type>billboard</type>
	<object-name>RedSpot.001</object-name>
	<object-name>RedSpot.002</object-name>
	<object-name>RedSpot.003</object-name>
	<object-name>RedSpot.004</object-name>
    </animation>

    <animation>
	<type>dist-scale</type>
	<object-name>RedSpot.001</object-name>
	<object-name>RedSpot.002</object-name>
	<object-name>RedSpot.003</object-name>
	<object-name>RedSpot.004</object-name>
	<interpolation>
	    <entry>
		<ind>0</ind>
		<dep>0.4</dep>
	    </entry>
	    <entry>
		<ind>500</ind>
		<dep>0.8</dep>
	    </entry>
	    <entry>
		<ind>16000</ind>
		<dep>10</dep>
	    </entry>
	</interpolation>
    </animation>

</PropertyList>

	
